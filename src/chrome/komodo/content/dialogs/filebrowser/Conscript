Import(
    'cons',
    'mozChromeDir',
    'platform',
    'productType',
    'buildFlavour',
    'unsiloedPythonExe',
    'mozVersion',
    'withCrypto',
);

$cons->InstallInChrome('filebrowser.xul');
$cons->Preprocess("filebrowser.unprocessed.js", "filebrowser.js");
$cons->InstallInChrome('filebrowser.js');
$cons->Preprocess("filebrowseview.unprocessed.js", "filebrowseview.js");
$cons->InstallInChrome('filebrowseview.js');

